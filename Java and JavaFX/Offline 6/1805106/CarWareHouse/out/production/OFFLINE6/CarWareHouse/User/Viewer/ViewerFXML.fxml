      <Button fx:id="searchCarButton" alignment="CENTER" layoutX="505.0" layoutY="187.0" mnemonicParsing="false" onAction="#searchCarClicked" prefHeight="26.0" prefWidth="106.0" text="Search Car" />
      <ListView fx:id="carList" layoutX="7.0" layoutY="14.0" prefHeight="454.0" prefWidth="429.0" />
      <TextField fx:id="registrationField" layoutX="440.0" layoutY="29.0" promptText="Registration No." />
      <TextField fx:id="manufacturerField" layoutX="440.0" layoutY="84.0" promptText="Manufacturer" />
      <TextField fx:id="modelField" layoutX="440.0" layoutY="136.0" promptText="Model" />
   </children>
</AnchorPane>
