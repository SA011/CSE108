      <TextField fx:id="registrationField" layoutX="455.0" layoutY="191.0" promptText="Registration No." />
      <TextField fx:id="manufacturerField" layoutX="455.0" layoutY="241.0" promptText="Manufacturer" />
      <TextField fx:id="modelField" layoutX="455.0" layoutY="298.0" promptText="Model" />
      <Button fx:id="addCarButton" layoutX="455.0" layoutY="32.0" mnemonicParsing="false" onAction="#addCarClicked" prefHeight="26.0" prefWidth="171.0" text="Add Car" />
